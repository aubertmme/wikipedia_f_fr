{
	"name": "pipeline_wffr",
	"properties": {
		"activities": [
			{
				"name": "Copy_flatten_wffr",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 60,
					"retryIntervalInSeconds": 120,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "/sparql?format=json&query=SELECT%20?item%20?itemLabel%20(COUNT%20(?has_part_s_)%20as%20?count_has_part_s_)%20(COUNT%20(?part_of)%20as%20?count_part_of)%20WHERE%20{%20?item%20wdt:P21%20wd:Q6581072;%20wdt:P1412%20wd:Q150.%20SERVICE%20wikibase:label%20{%20bd:serviceParam%20wikibase:language%20\"fr\".}%20OPTIONAL%20{%20?item%20wdt:P527%20?has_part_s_.%20}%20OPTIONAL%20{%20?item%20wdt:P361%20?part_of.%20}%20}%20GROUP%20BY%20?item%20?itemLabel"
					},
					{
						"name": "Destination",
						"value": "blobwikipediafrf/wikipedia_f_fr/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['head']['vars'][0]"
								},
								"sink": {
									"name": "items",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['results']['bindings'][0]['item']['value']"
								},
								"sink": {
									"name": "item",
									"type": "Byte"
								}
							},
							{
								"source": {
									"path": "$['results']['bindings'][0]['itemLabel']['value']"
								},
								"sink": {
									"name": "itemLabel",
									"type": "Byte"
								}
							},
							{
								"source": {
									"path": "$['results']['bindings'][0]['count_has_part_s_']['value']"
								},
								"sink": {
									"name": "count_has_part_s"
								}
							},
							{
								"source": {
									"path": "$['results']['bindings'][0]['count_part_of']['value']"
								},
								"sink": {
									"name": "count_part_of",
									"type": "Int16"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "json_wffr",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "parquet_wffr",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}