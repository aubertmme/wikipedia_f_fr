					"retryIntervalInSeconds": 120,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "/sparql?format=json&query=SELECT%20%3Fitem%20%3FitemLabel%0A(COUNT%20(%3Fhas_part_s_)%20as%20%3Fcount_has_part_s_)%0A(COUNT%20(%3Fpart_of)%20as%20%3Fcount_part_of)%20%0AWHERE%20%7B%0A%20%20%3Fitem%20wdt%3AP21%20wd%3AQ6581072%3B%20%23femmes%0A%20%20%20%20wdt%3AP1412%20wd%3AQ150.%20%23francophones%0A%20%20SERVICE%20wikibase%3Alabel%20%7B%20bd%3AserviceParam%20wikibase%3Alanguage%20%22fr%22.%7D%20%20%23wikidata%20francophone%0A%20%20OPTIONAL%20%7B%20%3Fitem%20wdt%3AP527%20%3Fhas_part_s_.%20%7D%0A%20%20OPTIONAL%20%7B%20%3Fitem%20wdt%3AP361%20%3Fpart_of.%20%7D%0A%7D%0AGROUP%20BY%20%3Fitem%20%3FitemLabel%0A"
					},
					{
						"name": "Destination",
						"value": "blobwikipediafrf/wikipedia_f_fr/"
